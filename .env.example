# This file is an example of environment variables for the Fantasy RAG Lab.
# You can copy this file to .env and fill in the values for your environment.

# OpenAI (Embeddings)
AZURE_OPENAI_ENDPOINT=https://<your-resource>.openai.azure.com/
AZURE_OPENAI_API_KEY=<your_azure_api_key>
AZURE_OPENAI_API_VERSION=2023-05-15
AZURE_OPENAI_EMBEDDING_DEPLOYMENT=<your_deployment_name>
AZURE_OPENAI_EMBEDDING_MODEL=text-embedding-ada-002
AZURE_OPENAI_EMBEDDING_DIMENSIONS=1536

# Google Gemini (LLM)
GEMINI_API_KEY=<your_gemini_api_key>
OPENAI_API_KEY=<your_openai_api_key>
ANTHROPIC_API_KEY=<your_anthropic_api_key>
VOYAGE_API_KEY=<your_voyage_api_key>
OPENROUTER_API_KEY=<your_openrouter_api_key>

# Zilliz Cloud (Vector DB)
MILVUS_ZILLIZ_URI=<your_cluster_endpoint>
MILVUS_ZILLIZ_TOKEN=<your_api_token>

# Neo4j Aura (Graph DB)
CLIENT_SECRET=<your_client_secret>
CLIENT_ID=<your_client_id>
CLIENT_NAME=<your_client_name>

# Project Settings
DATA_DIR=./data
CACHE_DIR=./cache

# LangSmith (Tracing)
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=<your_langsmith_api_key>
LANGSMITH_PROJECT=<your_project_name>

# MongoDB Altas
MongoDB=<>